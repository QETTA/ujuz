{
  "name": "ujuz-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:web": "next dev -p 3001",
    "dev:mobile": "bash ./scripts/start-mobile.sh",
    "dev:mobile:android": "bash ./scripts/start-mobile.sh --android",
    "dev:mobile:bg": "bash ./scripts/start-mobile.sh --bg",
    "dev:mobile:tunnel": "bash ./scripts/start-mobile.sh --tunnel",
    "check:mobile": "bash ./scripts/start-mobile.sh --check",
    "stop:mobile": "bash ./scripts/start-mobile.sh --stop",
    "url:mobile": "bash ./scripts/start-mobile.sh --url",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc -p tsconfig.json --noEmit",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "grants:bizinfo": "tsx scripts/grants/crawl-bizinfo.ts",
    "grants:prestartup": "tsx scripts/grants/crawl-bizinfo.ts --pre-startup-only 1 --maxPages 5 --sleepMs 200 --out docs/generated/grants/bizinfo.prestartup.json --md docs/generated/grants/bizinfo.prestartup.md",
    "grants:prestartup:full": "tsx scripts/grants/crawl-bizinfo.ts --pre-startup-only 1 --maxPages 5 --sleepMs 200 --downloadDir docs/generated/grants/attachments --extractText 1 --out docs/generated/grants/bizinfo.prestartup.full.json --md docs/generated/grants/bizinfo.prestartup.full.md"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.43",
    "@ai-sdk/react": "^3.0.88",
    "@anthropic-ai/sdk": "^0.74.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "ai": "^6.0.86",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fastify": "^5.5.0",
    "jose": "^6.1.3",
    "jstat": "^1.9.6",
    "lucide-react": "^0.563.0",
    "luxon": "^3.7.1",
    "mongodb": "^6.21.0",
    "motion": "^12.34.0",
    "next": "^15.1.0",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "^7.0.3",
    "postcss": "^8.5.6",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.18.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "mongodb-memory-server": "^10.3.0",
    "shadcn": "^3.8.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
