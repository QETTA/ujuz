{
  "name": "ujuz-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bash ./scripts/start-mobile.sh",
    "dev:mobile": "bash ./scripts/start-mobile.sh",
    "dev:mobile:android": "bash ./scripts/start-mobile.sh --android",
    "dev:mobile:bg": "bash ./scripts/start-mobile.sh --bg",
    "dev:mobile:tunnel": "bash ./scripts/start-mobile.sh --tunnel",
    "check:mobile": "bash ./scripts/start-mobile.sh --check",
    "stop:mobile": "bash ./scripts/start-mobile.sh --stop",
    "url:mobile": "bash ./scripts/start-mobile.sh --url",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc -p tsconfig.json --noEmit",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "grants:bizinfo": "tsx scripts/grants/crawl-bizinfo.ts",
    "grants:prestartup": "tsx scripts/grants/crawl-bizinfo.ts --pre-startup-only 1 --maxPages 5 --sleepMs 200 --out docs/generated/grants/bizinfo.prestartup.json --md docs/generated/grants/bizinfo.prestartup.md",
    "grants:prestartup:full": "tsx scripts/grants/crawl-bizinfo.ts --pre-startup-only 1 --maxPages 5 --sleepMs 200 --downloadDir docs/generated/grants/attachments --extractText 1 --out docs/generated/grants/bizinfo.prestartup.full.json --md docs/generated/grants/bizinfo.prestartup.full.md"
  },
  "dependencies": {
    "fastify": "^5.5.0",
    "jose": "^6.1.2",
    "luxon": "^3.7.1",
    "mongodb": "^6.21.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "mongodb-memory-server": "^10.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
